    "manual_preview_categories": {
      "type": "json"
    },
    "manual_review_categories": {
      "type": "json"
    },
    "enabled_where_to_watch": {
      "type": "boolean",
      "default": false
    },
    "where_to_watch_days": {
      "type": "integer",
      "default": 0
    },
    "where_to_watch_countries": {
      "type": "json"
    },
    "where_to_watch_categories": {
      "type": "json"
    }
  }
