      "inversedBy": "users_websites"
    },
    "featured_image": {
      "type": "enumeration",
      "default": "upload",
      "enum": [
        "upload",
        "url"
      ]
    },
    "l_version": {
      "type": "enumeration",
      "default": "eng",
      "enum": [
        "eng",
        "ru"
      ]
    },
    "is_validated": {
      "type": "boolean",
      "default": false
