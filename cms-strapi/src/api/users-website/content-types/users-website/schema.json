    "platform_name": {
      "type": "string"
    },
    "platform_url": {
      "type": "string"
    },
    "platform_user": {
      "type": "string"
    },
    "platform_password": {
      "type": "string"
    },
    "website_author": {
      "type": "json"
    },
    "active": {
      "type": "boolean",
      "default": false
    },
    "type_status": {
      "type": "enumeration",
      "default": "publish",
      "enum": [
        "publish",
        "draft"
      ]
    },
    "openai_prompt": {
      "type": "text"
    },
    "platform_countries": {
      "type": "json"
    },
    "website_leagues": {
      "type": "json"
    },
    "enable_match_reviews": {
      "type": "boolean"
    },
    "enable_match_previews": {
      "type": "boolean"
    },
    "match_reviews_time": {
      "type": "integer"
    },
    "match_previews_time": {
      "type": "integer"
    },
    "twitter_handles": {
      "type": "json"
    },
    "social_media_categories": {
      "type": "json"
    },
    "enable_social_media": {
      "type": "boolean"
    },
    "enabled_player_abroad": {
      "type": "boolean"
    },
    "player_abroad_prompt": {
      "type": "text"
    },
    "player_abroad_categories": {
      "type": "json"
    },
    "player_abroad_countries": {
      "type": "json"
    },
    "enable_player_profiles": {
      "type": "boolean"
    },
    "player_profiles": {
      "type": "json"
    },
    "player_profiles_categories": {
      "type": "json"
    },
    "enable_transfer_rumors": {
      "type": "boolean"
    },
